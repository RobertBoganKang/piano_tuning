Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"purify", "[", "x_", "]"}], ":=", 
   RowBox[{"(", "\n", 
    RowBox[{
     RowBox[{"wavImport", "=", 
      RowBox[{"Import", "[", 
       RowBox[{
        RowBox[{"wavDirectory", "<>", 
         RowBox[{"wavNames", "[", 
          RowBox[{"[", "x", "]"}], "]"}]}], ",", "\"\<Sound\>\""}], "]"}]}], 
     ";", "\n", 
     RowBox[{"wavIdealFreq", "=", 
      RowBox[{"num2freq", "[", 
       RowBox[{"noteNums", "[", 
        RowBox[{"[", "x", "]"}], "]"}], "]"}]}], ";", "\n", 
     RowBox[{"wavNoteNum", "=", 
      RowBox[{"noteNums", "[", 
       RowBox[{"[", "x", "]"}], "]"}]}], ";", "\n", 
     RowBox[{"wavNoteFreq", "=", 
      RowBox[{"num2freq", "[", "wavNoteNum", "]"}]}], ";", "\n", 
     RowBox[{"wavData", "=", 
      RowBox[{"wavImport", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\n", 
     RowBox[{"wavLength", "=", 
      RowBox[{"Length", "[", 
       RowBox[{"wavData", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\n", 
     RowBox[{"wavLength", "=", 
      RowBox[{
       RowBox[{"Floor", "[", 
        RowBox[{"wavLength", "/", "2"}], "]"}], "*", "2"}]}], ";", "\n", 
     RowBox[{"wavSampleRate", "=", 
      RowBox[{"wavImport", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ";", "\n", "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"input", ":", " ", 
        RowBox[{"audio", " ", "data"}]}], ";", " ", 
       RowBox[{"output", ":", " ", 
        RowBox[{"stretched", " ", "audio", " ", "data"}]}]}], "*)"}], "\n", 
     RowBox[{
      RowBox[{"stretchFourier", "[", "s_", "]"}], ":=", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"wavFourierFreqRe", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"i", "-", "1"}], ")"}], "*", 
              RowBox[{"wavSampleRate", "/", "wavLength"}]}], ",", 
             RowBox[{"Re", "[", 
              RowBox[{"s", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "s", "]"}]}], "}"}]}], "]"}]}], ";", "\n", 
        RowBox[{"wavFourierFreqIm", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"i", "-", "1"}], ")"}], "*", 
              RowBox[{"wavSampleRate", "/", "wavLength"}]}], ",", 
             RowBox[{"Im", "[", 
              RowBox[{"s", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "s", "]"}]}], "}"}]}], "]"}]}], ";", "\n", 
        RowBox[{"wavInterpolationRe", "=", 
         RowBox[{"Interpolation", "[", "wavFourierFreqRe", "]"}]}], ";", "\n", 
        RowBox[{"wavInterpolationIm", "=", 
         RowBox[{"Interpolation", "[", "wavFourierFreqIm", "]"}]}], ";", "\n",
         "\n", 
        RowBox[{"(*", 
         RowBox[{"tune", " ", "to", " ", "standard", " ", "frequency"}], 
         "*)"}], "\n", 
        RowBox[{"wavNoteRealFreq", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"freqTable", "[", "wavNoteNum", "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}], "/", 
          RowBox[{"ihFunction", "[", 
           RowBox[{"wavNoteNum", ",", 
            RowBox[{
             RowBox[{"freqTable", "[", "wavNoteNum", "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}]}], ";", "\n", 
        RowBox[{"wavStepFreqSize", "=", 
         RowBox[{"wavSampleRate", "/", "wavLength"}]}], ";", "\n", "\n", 
        RowBox[{"(*", "loop", "*)"}], "\n", 
        RowBox[{"i", "=", "0"}], ";", "\n", 
        RowBox[{"j", "=", "1"}], ";", "\n", 
        RowBox[{"loopEnd", "=", 
         RowBox[{"wavFourierFreqIm", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}]}], ";", "\n", 
        RowBox[{"arrRe", "=", 
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "wavLength"}], "}"}]}], "]"}]}], ";", "\n", 
        RowBox[{"arrIm", "=", 
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "wavLength"}], "}"}]}], "]"}]}], ";", "\n", 
        RowBox[{"While", "[", 
         RowBox[{"True", ",", "\n", 
          RowBox[{
           RowBox[{"temp0", "=", 
            RowBox[{"wavNoteFreq", "*", 
             RowBox[{"ihFunction", "[", 
              RowBox[{"wavNoteNum", ",", 
               RowBox[{"i", "/", "wavNoteRealFreq"}]}], "]"}]}]}], ";", "\n", 
           
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"temp0", ">", "loopEnd"}], ",", 
             RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\n", 
           RowBox[{
            RowBox[{"arrRe", "[", 
             RowBox[{"[", "j", "]"}], "]"}], "=", 
            RowBox[{"wavInterpolationRe", "[", "temp0", "]"}]}], ";", "\n", 
           RowBox[{
            RowBox[{"arrIm", "[", 
             RowBox[{"[", "j", "]"}], "]"}], "=", 
            RowBox[{"wavInterpolationIm", "[", "temp0", "]"}]}], ";", "\n", 
           RowBox[{"i", "+=", "wavStepFreqSize"}], ";", "\n", 
           RowBox[{"j", "+=", "1"}]}]}], "]"}], ";", "\n", 
        RowBox[{
         RowBox[{"arrRe", "[", 
          RowBox[{"[", 
           RowBox[{";;", 
            RowBox[{"Round", "[", 
             RowBox[{"wavLength", "/", "2"}], "]"}]}], "]"}], "]"}], "+", 
         RowBox[{
          RowBox[{"arrIm", "[", 
           RowBox[{"[", 
            RowBox[{";;", 
             RowBox[{"Round", "[", 
              RowBox[{"wavLength", "/", "2"}], "]"}]}], "]"}], "]"}], "*", 
          "I"}]}]}], ")"}]}], ";", "\n", "\n", 
     RowBox[{
      RowBox[{"wavStretch", "[", "wavData_", "]"}], ":=", 
      RowBox[{"(", "\n", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"OddQ", "[", 
           RowBox[{"Length", "[", "wavData", "]"}], "]"}], ",", 
          RowBox[{"temp", "=", 
           RowBox[{"Drop", "[", 
            RowBox[{"wavData", ",", 
             RowBox[{"-", "1"}]}], "]"}]}]}], "]"}], ";", "\n", 
        RowBox[{"wavFourier", "=", 
         RowBox[{"Fourier", "[", "temp", "]"}]}], ";", "\n", 
        RowBox[{"wavFourier0", "=", 
         RowBox[{"wavFourier", "[", 
          RowBox[{"[", 
           RowBox[{";;", 
            RowBox[{"Round", "[", 
             RowBox[{"wavLength", "/", "2"}], "]"}]}], "]"}], "]"}]}], ";", 
        "\n", 
        RowBox[{"wavFourier1", "=", 
         RowBox[{
          RowBox[{"Reverse", "[", "wavFourier", "]"}], "[", 
          RowBox[{"[", 
           RowBox[{";;", 
            RowBox[{"Round", "[", 
             RowBox[{"wavLength", "/", "2"}], "]"}]}], "]"}], "]"}]}], ";", 
        "\n", 
        RowBox[{"(*", 
         RowBox[{"reconstruct", " ", "stretched", " ", "data"}], "*)"}], "\n", 
        RowBox[{"wavFourierReconstructed0", "=", 
         RowBox[{"stretchFourier", "[", "wavFourier0", "]"}]}], ";", "\n", 
        RowBox[{"wavFourierReconstructed1", "=", 
         RowBox[{"stretchFourier", "[", "wavFourier1", "]"}]}], ";", "\n", 
        RowBox[{"wavFourierReconstructed", "=", 
         RowBox[{"Flatten", "[", 
          RowBox[{"{", 
           RowBox[{"wavFourierReconstructed0", ",", 
            RowBox[{"Reverse", "[", "wavFourierReconstructed1", "]"}]}], 
           "}"}], "]"}]}], ";", "\n", 
        RowBox[{"wavReconstruct", "=", 
